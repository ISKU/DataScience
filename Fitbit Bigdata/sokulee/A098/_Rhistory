path <- "/home/hyunholee/data_analize/data"
dir_list <- list.files(path = ".", pattern = NULL, all.files = FALSE, full.names = FALSE, recursive = FALSE)
for(dir in dir_list){
# set the directory
print(dir)
path_dir <- paste(path,dir,sep="/")
print(path_dir)
#set the file
file_list <- list.files(path =path_dir, pattern = NULL, all.files = FALSE, full.names = FALSE, recursive = FALSE)
print(file_list)
for(files in file_list){
# only step file
setwd(path_dir)
# print(files)
if(step_file <- grepl('steps.json',files)){
data <- fromJSON(file=files)
total_data <- data$`activities-step`
}
}
}
